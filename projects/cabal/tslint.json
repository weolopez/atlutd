{
  "extends": [
    "../../tslint.json",
    "@phenomnomnominal/angular-lazy-routes-fix"
  ],
  "no-lazy-module-paths": [true, "async"],
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}